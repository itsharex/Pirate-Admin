{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "generateOptions": {
    "spec": false
  },
  "compilerOptions": {
    "webpack": true,
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": [
      "**/*.env"
    ]
  }
}
